<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <WebPublishMethod>MSDeploy</WebPublishMethod>
    <LastUsedBuildConfiguration>Release</LastUsedBuildConfiguration>
    <LastUsedPlatform>Any CPU</LastUsedPlatform>
    <SiteUrlToLaunchAfterPublish>http://bchen-mvc.azurewebsites.net</SiteUrlToLaunchAfterPublish>
    <LaunchSiteAfterPublish>True</LaunchSiteAfterPublish>
    <ExcludeApp_Data>False</ExcludeApp_Data>
    <MSDeployServiceURL>bchen-mvc.scm.azurewebsites.net:443</MSDeployServiceURL>
    <DeployIisAppPath>bchen-mvc</DeployIisAppPath>
    <RemoteSitePhysicalPath />
    <SkipExtraFilesOnServer>True</SkipExtraFilesOnServer>
    <MSDeployPublishMethod>WMSVC</MSDeployPublishMethod>
    <EnableMSDeployBackup>True</EnableMSDeployBackup>
    <UserName>$bchen-mvc</UserName>
    <_SavePWD>True</_SavePWD>
    <_DestinationType>AzureWebSite</_DestinationType>
    <PublishDatabaseSettings>
      <Objects xmlns="">
        <ObjectGroup Name="DefaultConnection" Order="1" Enabled="False">
          <Destination Path="Data Source=tcp:bochen2014.database.windows.net,1433;Initial Catalog=userDB;User ID=bochen2014@bochen2014.database.windows.net;Password=Password01" Name="Data Source=tcp:bochen2014.database.windows.net,1433;Initial Catalog=userDB;User Id=bochen2014@bochen2014.database.windows.net;Password=Password01" />
          <Object Type="DbCodeFirst">
            <Source Path="DBMigration" DbContext="mvcSample.Models.ApplicationDbContext, mvcSample" MigrationConfiguration="mvcSample.Migrations.Configuration, mvcSample" Origin="Convention" />
          </Object>
        </ObjectGroup>
      </Objects>
    </PublishDatabaseSettings>
  </PropertyGroup>
  <ItemGroup>
    <MSDeployParameterValue Include="$(DeployParameterPrefix)DefaultConnection-Web.config Connection String">
      <ParameterValue>Data Source=tcp:bochen2014.database.windows.net,1433;Initial Catalog=userDB;User Id=bochen2014@bochen2014.database.windows.net;Password=Password01</ParameterValue>
    </MSDeployParameterValue>
  </ItemGroup>
</Project>